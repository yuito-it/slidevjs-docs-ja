    "vue": "^3.2.30",
    "windicss": "^3.4.3",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/plantuml-encoder": "^1.4.0",
    "fast-deep-equal": "^3.1.3",
    "semver": "^7.3.5",
    "sirv": "^2.0.2"
  },
