    "vite": "^2.2.2",
    "vite-plugin-components": "^0.8.4",
    "vite-plugin-icons": "^0.5.0",
    "vite-plugin-md": "^0.6.3",
    "vite-plugin-remote-assets": "^0.1.4",
    "vite-plugin-windicss": "^0.15.7",
    "vue": "^3.0.11",
    "windicss": "^2.5.14",
    "yargs": "^16.2.0"
