    "playwright": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/utils": "^0.1.0",
    "@iconify/json": "^1.1.333",
    "@slidev/client": "workspace:*",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.11",
    "codemirror": "^5.61.0",
    "enquirer": "^2.3.6",
