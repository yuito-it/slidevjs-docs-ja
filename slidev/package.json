{
  "name": "@slidev/cli",
  "version": "0.28.4",
  "description": "Presentation slides for developers",
  "homepage": "https://sli.dev",
  "bugs": "https://github.com/slidevjs/slidev/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/slidevjs/slidev"
  },
  "funding": "https://github.com/sponsors/antfu",
  "author": "antfu <anthonyfu117@hotmail.com>",
  "files": [
    "bin",
    "client",
    "dist",
    "template.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "slidev": "./bin/slidev.js"
  },
  "scripts": {
    "build": "rimraf dist && tsup node/index.ts node/cli.ts --dts --format cjs,esm --splitting --external @slidev/client/package.json",
    "dev": "nr build --watch",
    "prepublishOnly": "npm run build",
    "start": "esno node/cli.ts"
  },
  "peerDependencies": {
    "playwright-chromium": "^1.10.0"
  },
  "peerDependenciesMeta": {
    "playwright-chromium": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/utils": "^0.5.0",
    "@iconify-json/carbon": "^1.0.14",
    "@iconify-json/ph": "^1.0.4",
    "@slidev/client": "workspace:*",
    "@slidev/parser": "workspace:*",
    "@slidev/types": "workspace:*",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/compiler-sfc": "^3.2.30",
    "cli-progress": "^3.10.0",
    "codemirror": "^5.65.1",
    "connect": "^3.7.0",
    "debug": "^4.3.3",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.0",
    "global-dirs": "^3.0.0",
    "import-from": "^4.0.0",
    "is-installed-globally": "^0.4.0",
    "jiti": "^1.12.15",
    "js-base64": "^3.7.2",
    "katex": "^0.15.2",
    "kolorist": "^1.5.1",
    "markdown-it": "^12.3.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-link-attributes": "^4.0.0",
    "monaco-editor": "^0.32.1",
    "nanoid": "^3.2.0",
    "open": "^8.4.0",
    "pdf-lib": "^1.17.1",
    "plantuml-encoder": "^1.4.0",
    "prismjs": "^1.26.0",
